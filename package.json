{
  "name": "plinko-extension",
  "displayName": "Plinko Simulator",
  "description": "A fun Plinko ball simulation game in VSCode",
  "version": "1.0.0",
  "publisher": "k4cper-g",
  "license": "MIT",
  "icon": "src/plinko-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/k4cper-g/plinko-vs"
  },
  "bugs": {
    "url": "https://github.com/k4cper-g/plinko-vs/issues"
  },
  "homepage": "https://github.com/k4cper-g/plinko-vs#readme",
  "keywords": [
    "game",
    "plinko",
    "fun",
    "entertainment",
    "simulator",
    "gambling"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "plinko.start",
        "title": "Plinko: Start Game"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "typescript": "^5.0.4"
  }
}
